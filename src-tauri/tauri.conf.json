{
  "$schema": "https://raw.githubusercontent.com/nickel-org/nickel.rs/master/schema.json",
  "productName": "Margin",
  "version": "0.1.0",
  "identifier": "com.margin.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "title": "Margin",
    "windows": [
      {
        "title": "Margin",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "dialog": {},
    "fs": {
      "scope": {
        "allow": ["**"],
        "deny": []
      }
    },
    "sql": {}
  }
}
